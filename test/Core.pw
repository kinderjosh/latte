// Printing

Void PutString(String s)
{
    __C__("fputs(s_, stdout);");
}

Void PutChar(Char c)
{
    __C__("fputc(c_, stdout);");
}

Void PutInt(Int i)
{
    __C__("printf(\"%d\", i_);");
}

Void PutFloat(Float f)
{
    __C__("printf(\"%f\", f_);");
}